{
  "name": "ideanick",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prettify": "prettier --log-level log --cache --write {webapp,backend}/src/**/*.{ts,tsx,js,json,yml,scss} !**/{node_modules,dist}/**/*",
    "b": "pnpm --filter @ideanick/backend",
    "w": "pnpm --filter @ideanick/webapp",
    "dev": "pnpm --r --parallel dev",
    "types": "concurrently \"pnpm b types\" \"pnpm w types\"",
    "types2": "pnpm b types && pnpm w types"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "3.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}